[
  {
    "Key": "PluginName",
    "Type": "string",
    "DefaultValue": "AzureDevOps"
  },
  {
    "Key": "Organization",
    "Type": "string",
    "DefaultValue": "your-organization"
  },
  {
    "Key": "Project",
    "Type": "string",
    "DefaultValue": "your-project"
  },
  {
    "Key": "PersonalAccessToken",
    "Type": "string",
    "DefaultValue": "your-personal-access-token"
  },
  {
    "Key": "AreaPath",
    "Type": "string",
    "DefaultValue": "your-project\\TeamName\\AreaPath"
  },
  {
    "Key": "ApiEndpoint",
    "Type": "string",
    "DefaultValue": "https://dev.azure.com"
  },
  {
    "Key": "Timeout",
    "Type": "int",
    "DefaultValue": "30"
  },
  {
    "Key": "DbPath",
    "Type": "string",
    "DefaultValue": "azuredevops.db"
  }
]
